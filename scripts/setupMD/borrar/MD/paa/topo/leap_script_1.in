LIG = loadpdb /borrar/MD/paa/topo/paa_lig.pdb
REC = loadpdb /borrar/MD/paa/topo/paa_rec.pdb
COM = loadpdb /borrar/MD/paa/topo/paa_com.pdb

loadAmberParams /borrar/MD/paa/lib/paa.frcmod

saveAmberParm LIG /borrar/MD/paa/topo/paa_vac_lig.parm7 /borrar/MD/paa/topo/paa_vac_lig.crd
