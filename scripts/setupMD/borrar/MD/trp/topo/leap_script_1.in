source leaprc.protein.ff14SB
source leaprc.water.tip3p
source leaprc.gaff

loadoff /borrar/MD/trp/lib/trp.lib
loadoff /borrar/MD/cofactor_lib/ihp.lib

LIG = loadpdb /borrar/MD/trp/topo/trp_lig.pdb
REC = loadpdb /borrar/MD/trp/topo/trp_rec.pdb
COM = loadpdb /borrar/MD/trp/topo/trp_com.pdb

loadAmberParams /borrar/MD/trp/lib/trp.frcmod

saveAmberParm LIG /borrar/MD/trp/topo/trp_vac_lig.parm7 /borrar/MD/trp/topo/trp_vac_lig.crd
savepdb LIG /borrar/MD/trp/topo/trp_vac_lig.pdb

saveAmberParm REC /borrar/MD/trp/topo/trp_vac_rec.parm7 /borrar/MD/trp/topo/trp_vac_rec.crd
savepdb REC /borrar/MD/trp/topo/trp_vac_rec.pdb

saveAmberParm COM /borrar/MD/trp/topo/trp_vac_com.parm7 /borrar/MD/trp/topo/trp_vac_com.crd
savepdb COM /borrar/MD/trp/topo/trp_vac_com.pdb

charge LIG
charge REC
charge COM

quit
